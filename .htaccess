RewriteRule ^([^/]+)/order?$ pages/my-order.php?store=$1 [L,QSA]
RewriteRule ^([^/]+)/vouchers?$ pages/voucher-list.php?store=$1 [L,QSA]
RewriteRule ^([^/]+)/my-vouchers?$ pages/my-vouchers.php?store=$1 [L,QSA]
RewriteRule ^([^/]+)/store-qr?$ pages/store-qr.php?store=$1 [L,QSA]
RewriteRule ^([^/]+)/$ pages/?store=$1 [L,QSA]
